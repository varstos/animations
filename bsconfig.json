{
  "name": "animations",
  "refmt": 3,
  "reason": {
    "react-jsx": 3
  },
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "suffix": ".bs.js",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "reason-shims",
      "subdirs": true
    }
  ],
  "bs-dependencies": [
    "reason-react",
    "reason-react-native",
    "@reason-react-native/safe-area-context",
  ],

  "gentypeconfig": {
    "language": "typescript",
    "shims": {
      "ReactNative": "ReactNative",
      "Js": "Js"
    },
    "importPath": "relative",
    "debug": {
      "all": false,
      "basic": false
    }
  }
}
